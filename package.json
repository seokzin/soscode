{
  "name": "soscode",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn config:build && docusaurus start",
    "build": "yarn config:build && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "test": "jest",
    "test:watch": "jest --watch ",
    "config:build": "yarn esbuild docusaurus.config.ts --bundle --platform=node --outfile=docusaurus.config.js --external:\"./sidebars.ts\" --external:\"*.css\" --external:esbuild-loader"
  },
  "dependencies": {
    "@docusaurus/core": "2.1.0",
    "@docusaurus/preset-classic": "2.1.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/jest": "^29.2.0",
    "esbuild": "^0.15.10",
    "esbuild-loader": "^2.20.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
