{
  "name": "soscode",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "docusaurus": "docusaurus",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "start": "yarn config:build && docusaurus start",
    "build": "yarn config:build && docusaurus build",
    "clean": "rm -rf yarn.lock node_modules",
    "reinstall": "yarn clean && yarn install",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "test": "jest",
    "test:watch": "jest --watch ",
    "config:build": "yarn esbuild docusaurus.config.ts --bundle --platform=node --outfile=docusaurus.config.js --external:\"./sidebars.ts\" --external:\"*.css\" --external:esbuild-loader"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
